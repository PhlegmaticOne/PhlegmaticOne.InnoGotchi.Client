@model IList<PreviewFarmViewModel>
@{
    ViewData["Title"] = "Collaborated Farms";
}


@if (Model.Count == 0)
{
    @await Html.PartialAsync("~/Views/_Partial_Views/Shared/PageTitle.cshtml", "You haven't collaborated farms")
}
else
{
    @await Html.PartialAsync("~/Views/_Partial_Views/Shared/PageTitle.cshtml", "Collaborated farms")
    @await Html.PartialAsync("~/Views/_Partial_Views/Farms/FarmPreviewsList.cshtml", Model)
}